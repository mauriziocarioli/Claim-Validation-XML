package citrite;

import java.lang.Number;

rule "Validation - 30 days" extends "Is Follow On Order"
	dialect "mvel"
	when
		>bre : BREObjectSFDC( orderID : request.order.orderHeader.orderID, request.order.orderHeader.orderCreationDate, d : request.rewardRegistration)
		>function : Helper( )
	then
		>function.registerValidation( bre, orderID, "Minimum Follow On Order Timeline Unmet", "STANDARD", "FAIL", "true" );
end
