package citrite;

import java.lang.Number;

rule "Validation - 30 days" extends "Is Follow On Order"
	dialect "mvel"
	when
		>bre : BREObjectSFDC( orderID : request.order.orderHeader.orderID, rd : request.rewardRegistration, cd : request.order.orderHeader.orderCreationDate)
		>function : Helper( )
		cd is before rd
	then
		>function.registerValidation( bre, orderID, "Minimum Follow On Order Timeline Unmet", "STANDARD", "FAIL", "true" );
end
